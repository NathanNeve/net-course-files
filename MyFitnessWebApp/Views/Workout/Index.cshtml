@{

}
<h1>Workouts</h1>
<h3>A list of future workouts</h3>

@if (true) {
    <ol>
        @foreach (var workout in (IList<string>)ViewData["Workouts"])
        {
            <li>@workout</li>
        }
    </ol>
    <ol>
        @foreach (var gym in ViewBag.Gyms)
        {
            <li>@gym.Name</li>
            <li>@gym.Adress</li>
            <li>@gym.MaxCapacity</li>
        }
    </ol>
}